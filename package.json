{
  "name": "prolink-tools",
  "version": "0.0.1",
  "description": "A collection of userfriendly tools for DJs using Pioneers PRO DJ LINK feature",
  "author": "Evan Purkhiser <evanpurkhiser@gmail.com>",
  "license": "MIT",
  "main": "./dist/main/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production tsc",
    "build-renderer": "cross-env NODE_ENV=production webpack",
    "build": "yarn run build-main && yarn run build-renderer",
    "start-renderer-dev": "webpack-dev-server",
    "start-main-dev": "tsc --watch && electron ./dist/main.js",
    "start": "electron .",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder"
  },
  "build": {
    "productName": "Prolink Tools",
    "appId": "com.evanpurkhiser.tools.prolink",
    "mac": {
      "category": "public.app-category.music"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.2",
    "@types/jest": "^25.2.2",
    "@types/node": "12",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webdriverio": "^5.0.0",
    "@types/webpack-dev-server": "^3.10.1",
    "@types/webpack-env": "^1.13.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.1",
    "electron": "^9.0.0",
    "electron-builder": "^22.3.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.7",
    "null-loader": "^4.0.0",
    "prettier": "^2.0.5",
    "react-refresh": "^0.8.3",
    "react-test-renderer": "^16.8.6",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/deep-diff": "^1.0.0",
    "@types/module-alias": "^2.0.0",
    "@types/object-path": "^0.11.0",
    "deep-diff": "^1.0.2",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "filesize": "^6.1.0",
    "framer-motion": "^1.11.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-utils": "^5.5.7",
    "module-alias": "^2.2.2",
    "object-path": "^0.11.4",
    "prolink-connect": "0.1.0-prerelease.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^2.0.8",
    "serializr": "^2.0.3",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.1"
  },
  "volta": {
    "node": "13.12.0"
  }
}
